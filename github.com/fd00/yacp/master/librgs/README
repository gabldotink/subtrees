  /usr/include/librgs/rgs_data.h
  /usr/include/librgs/rgs_label.h
  /usr/include/librgs/rgs_screen.h
  /usr/include/librgs/rgs_shape.h
  /usr/include/librgs/rgs_sound.h
  /usr/include/librgs/rgs_sprite.h
  /usr/include/librgs/rgs_surface.h
  /usr/include/librgs/rgs_tilemap.h
  /usr/include/librgs/rgs_ui.h
  /usr/include/librgs/smallfont.h
