  /usr/share/man/man1/pnetcdf_version.1.gz

(libpnetcdf4)
  /usr/bin/cygpnetcdf-4.dll

(libpnetcdf-devel)
  /usr/bin/pnetcdf-config
  /usr/include/pnetcdf
  /usr/include/pnetcdf.h
  /usr/lib/libpnetcdf.dll.a
  /usr/lib/pkgconfig/pnetcdf.pc
