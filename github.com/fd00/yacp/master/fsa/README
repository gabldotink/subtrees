  /usr/bin/map_gff_coords.exe
  /usr/bin/percentid.exe
  /usr/bin/prot2codon.exe
  /usr/bin/slice_fasta.exe
  /usr/bin/slice_fasta_gff.exe
  /usr/bin/slice_mercator_alignment.exe
  /usr/bin/translate.exe
  /usr/share/doc/Cygwin/fsa.README
  /usr/share/doc/fsa/COPYING
  /usr/share/doc/fsa/LICENSE
  /usr/share/doc/fsa/README

