  /usr/share/man/man1/cabocha.1.gz

(libcabocha5)
  /usr/bin/cygcabocha-5.dll

(libcabocha-devel)
  /usr/bin/cabocha-config
  /usr/include/cabocha.h
  /usr/lib/libcabocha.dll.a
  /usr/lib/pkgconfig/cabocha.pc

