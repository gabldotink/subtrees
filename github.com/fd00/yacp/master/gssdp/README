  /usr/bin/cyggssdp-1.2-0.dll

(libgssdp1.2-devel)
  /usr/include/gssdp-1.2/libgssdp/gssdp-client.h
  /usr/include/gssdp-1.2/libgssdp/gssdp-enums.h
  /usr/include/gssdp-1.2/libgssdp/gssdp-error.h
  /usr/include/gssdp-1.2/libgssdp/gssdp-resource-browser.h
  /usr/include/gssdp-1.2/libgssdp/gssdp-resource-group.h
  /usr/include/gssdp-1.2/libgssdp/gssdp.h
  /usr/lib/libgssdp-1.2.dll.a
  /usr/lib/pkgconfig/gssdp-1.2.pc

(girepository-GSSDP1.2)
  /usr/lib/girepository-1.0/GSSDP-1.2.typelib
  /usr/share/gir-1.0/GSSDP-1.2.gir

(vala-gssdp1.2)
  /usr/share/vala/vapi/gssdp-1.2.deps
  /usr/share/vala/vapi/gssdp-1.2.vapi

