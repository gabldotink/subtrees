  /usr/share/doc/libqsearch/THANKS
  /usr/share/man/man1/maketree.1.gz
  /usr/share/man/man1/qsearch-config.1.gz

(libqsearch-gomp1)
  /usr/bin/cygqsearch-gomp-1.dll

(libqsearch-gomp-devel)
  /usr/bin/qsearch-config.exe
