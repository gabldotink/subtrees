  /usr/include/libfolia/folia_engine.h
  /usr/include/libfolia/folia_impl.h
  /usr/include/libfolia/folia_metadata.h
  /usr/include/libfolia/folia_properties.h
  /usr/include/libfolia/folia_provenance.h
  /usr/include/libfolia/folia_subclasses.h
  /usr/include/libfolia/folia_textpolicy.h
  /usr/include/libfolia/folia_types.h
  /usr/include/libfolia/folia_utils.h
  /usr/lib/libfolia.dll.a
  /usr/lib/pkgconfig/folia.pc

