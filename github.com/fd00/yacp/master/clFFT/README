  /usr/share/clFFT/examples/fft1d.exe
  /usr/share/clFFT/examples/fft2d.c
  /usr/share/clFFT/examples/fft2d.exe
  /usr/share/clFFT/examples/fft3d.c
  /usr/share/clFFT/examples/fft3d.exe

