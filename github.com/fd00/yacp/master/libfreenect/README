  /usr/bin/freenect-micview.exe
  /usr/bin/freenect-regtest.exe
  /usr/bin/freenect-regview.exe
  /usr/bin/freenect-tiltdemo.exe
  /usr/bin/freenect-wavrecord.exe
  /usr/share/doc/Cygwin/libfreenect.README
