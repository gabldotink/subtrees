  /usr/share/doc/corona/literature.txt
  /usr/share/doc/corona/readme.txt
  /usr/share/doc/corona/release-howto.txt
  /usr/share/doc/corona/tutorial.txt

(libcorona1.0.3)
  /usr/bin/cygcorona-1-0-3.dll

(libcorona-devel)
  /usr/bin/corona-config
