(python39-libbitcoin-consensus)
  /usr/lib/python3.9/site-packages/libbitcoin-consensus/__pycache__/consensus.cpython-39.opt-1.pyc
  /usr/lib/python3.9/site-packages/libbitcoin-consensus/__pycache__/consensus.cpython-39.opt-2.pyc
  /usr/lib/python3.9/site-packages/libbitcoin-consensus/__pycache__/consensus.cpython-39.pyc
  /usr/lib/python3.9/site-packages/libbitcoin-consensus/_bitcoin-consensus.dll
  /usr/lib/python3.9/site-packages/libbitcoin-consensus/consensus.py

