  /usr/share/doc/aws-c-event-stream/NOTICE
  /usr/share/doc/aws-c-event-stream/README.md

(libaws-c-event-stream0)
  /usr/bin/cygaws-c-event-stream-0.dll

(libaws-c-event-stream-devel)
  /usr/include/aws/event-stream/event_stream.h
  /usr/include/aws/event-stream/event_stream_exports.h
  /usr/lib/cmake/aws-c-event-stream/aws-c-event-stream-config.cmake
