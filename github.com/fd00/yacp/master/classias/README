  /usr/include/classias/classias.h
  /usr/include/classias/data.h
  /usr/include/classias/evaluation.h
  /usr/include/classias/feature_generator.h
  /usr/include/classias/instance.h
  /usr/include/classias/lbfgs.h
  /usr/include/classias/multi.h
  /usr/include/classias/online_scheduler.h
  /usr/include/classias/parameters.h
  /usr/include/classias/pegasos.h
  /usr/include/classias/quark.h
  /usr/include/classias/truncated_gradient.h
  /usr/include/classias/types.h
  /usr/include/classias/version.h
  /usr/share/doc/Cygwin/classias.README
  /usr/share/doc/classias/AUTHORS
  /usr/share/doc/classias/COPYING
  /usr/share/doc/classias/ChangeLog
  /usr/share/doc/classias/INSTALL
  /usr/share/doc/classias/README

