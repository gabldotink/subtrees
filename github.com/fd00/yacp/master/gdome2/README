  /usr/share/doc/gdome2/ChangeLog
  /usr/share/doc/gdome2/MAINTAINERS
  /usr/share/doc/gdome2/README

(libgdome0)
  /usr/bin/cyggdome-0.dll

(libgdome-devel)
  /usr/bin/gdome-config
  /usr/include/libgdome/gdome-events.h
  /usr/include/libgdome/gdome-libxml-util.h
  /usr/include/libgdome/gdome-private-list.h
  /usr/include/libgdome/gdome-refdebug.h
  /usr/include/libgdome/gdome-treegc.h
  /usr/include/libgdome/gdome-util.h
  /usr/include/libgdome/gdome-xpath.h
  /usr/include/libgdome/gdome.h
  /usr/lib/gdomeConf.sh
  /usr/lib/libgdome.dll.a
  /usr/lib/pkgconfig/gdome2.pc
  /usr/share/aclocal/gdome2.m4
  /usr/share/man/man1/gdome-config.1.gz

