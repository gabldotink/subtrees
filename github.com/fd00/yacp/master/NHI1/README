  /usr/share/doc/NHI1/COPYING
  /usr/share/doc/NHI1/ChangeLog
  /usr/share/doc/NHI1/NEWS
  /usr/share/doc/NHI1/README
  /usr/share/doc/NHI1/THANKS

(libmsgque22)
  /usr/bin/cygmsgque-22.dll

(libmsgque-devel)
  /usr/include/NHI1/msgque.h
  /usr/lib/libmsgque.dll.a

(libccmsgque22)
  /usr/bin/cygccmsgque-22.dll

(libccmsgque-devel)
  /usr/include/NHI1/ccmsgque.h
  /usr/lib/libccmsgque.dll.a

(python-NHI1)
  /usr/lib/NHI1/pymsgque.dll

(tcl-NHI1)
  /usr/lib/NHI1/pkgIndex.tcl
  /usr/lib/NHI1/tclmsgque.dll

