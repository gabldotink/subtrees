  /usr/share/snp-dists/test/bad.aln
  /usr/share/snp-dists/test/empty.aln
  /usr/share/snp-dists/test/good-c-m.res
  /usr/share/snp-dists/test/good-c.res
  /usr/share/snp-dists/test/good.aln
  /usr/share/snp-dists/test/good.res
  /usr/share/snp-dists/test/gzip.aln.gz
  /usr/share/snp-dists/test/gzip.res
  /usr/share/snp-dists/test/lowercase-k.res
  /usr/share/snp-dists/test/lowercase.aln
  /usr/share/snp-dists/test/lowercase.res
  /usr/share/snp-dists/test/singleton.aln
  /usr/share/snp-dists/test/singleton.res

