  /usr/include/chafa/chafa-version-macros.h
  /usr/include/chafa/chafa.h
  /usr/lib/chafa/include/chafaconfig.h
  /usr/lib/libchafa.dll.a
  /usr/lib/pkgconfig/chafa.pc

