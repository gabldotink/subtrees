  /usr/share/doc/guiloader-c++/msvc.ru.txt
  /usr/share/doc/guiloader-c++/news.en.txt
  /usr/share/doc/guiloader-c++/news.ru.txt
  /usr/share/doc/guiloader-c++/readme.en.txt
  /usr/share/doc/guiloader-c++/readme.ru.txt

(libguiloader-c++1)
  /usr/bin/cygguiloader-c++-1.dll
  /usr/share/locale/ru/LC_MESSAGES/guiloader-c++.mo

(libguiloader-c++-devel)
  /usr/include/guiloader-c++/guiloader-c++.h
  /usr/lib/libguiloader-c++.dll.a
  /usr/lib/pkgconfig/guiloader-c++.pc

