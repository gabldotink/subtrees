  /usr/include/libewf/features.h
  /usr/include/libewf/types.h
  /usr/lib/libewf.dll.a
  /usr/lib/pkgconfig/libewf.pc
  /usr/share/man/man3/libewf.3.gz

(python39-libewf)
  /usr/lib/python3.9/site-packages/pyewf.dll

