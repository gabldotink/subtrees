  /usr/share/man/man1/ewfrecover.1.gz
  /usr/share/man/man1/ewfverify.1.gz

(libewf3)
  /usr/bin/cygewf-3.dll

(libewf-devel)
  /usr/include/libewf.h
  /usr/include/libewf/codepage.h
  /usr/include/libewf/definitions.h
  /usr/include/libewf/error.h
