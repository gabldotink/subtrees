  /usr/share/doc/libewf/COPYING
  /usr/share/doc/libewf/COPYING.LESSER
  /usr/share/doc/libewf/ChangeLog
  /usr/share/doc/libewf/Expert Witness Compression Format (EWF).asciidoc
  /usr/share/doc/libewf/Expert Witness Compression Format 2 (EWF2).asciidoc
  /usr/share/doc/libewf/README
  /usr/share/man/man1/ewfacquire.1.gz
  /usr/share/man/man1/ewfacquirestream.1.gz
  /usr/share/man/man1/ewfexport.1.gz
  /usr/share/man/man1/ewfinfo.1.gz
