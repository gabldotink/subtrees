  /usr/include/logog/socket.hpp
  /usr/include/logog/statics.hpp
  /usr/include/logog/string.hpp
  /usr/include/logog/target.hpp
  /usr/include/logog/thread.hpp
  /usr/include/logog/timer.hpp
  /usr/include/logog/topic.hpp
  /usr/include/logog/unittest.hpp
  /usr/lib/liblogog.dll.a

