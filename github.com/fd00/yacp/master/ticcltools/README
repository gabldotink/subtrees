  /usr/share/man/man1/TICCL-mergelex.1.gz
  /usr/share/man/man1/TICCL-rank.1.gz
  /usr/share/man/man1/TICCL-stats.1.gz
  /usr/share/man/man1/TICCL-unk.1.gz

(libticcl1)
  /usr/bin/cygticcl-1.dll

(libticcl-devel)
